@using Form114.Infrastructure
@model Form114.Models.SearchViewModel
@{
    ViewBag.Title = "Recherche Appartement de Vacances";
}

@section scripts{
<script src="~/Scripts/jquery-2.1.4.js"></script>
<script src="~/Scripts/jquery-ui.js"></script>
    <script src="http://harvesthq.github.io/chosen/chosen.jquery.js"></script>
    <script src="~/Scripts/Search/Slider.js"></script>
<script src="~/Scripts/Search/ListeVille.js"></script>
<script src="~/Scripts/Search/Datepicker.js"></script>

<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<link href="~/Content/bootstrap-chosen.css" rel="stylesheet" />
}
<h2>Search</h2>


@using (Html.BeginForm("Result", "Search")){
    using (@Html.FieldsetHelper("Prix du Logement"))
    {
        @Html.Slider("slider-range")
    
        @Html.ListeVille()
    
        @Html.NombreDePlace()
        
        @Html.DatePicker()
    
    }

}
<div class="col-lg-4"></div>


